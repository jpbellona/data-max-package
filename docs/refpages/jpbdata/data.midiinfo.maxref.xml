<c74object category='controller, data' name='data.midiinfo' module='max, data'>
  <digest>
    Extends midiinfo
  </digest>
  <!--DESCRIPTION-->
  <description>
    <o>data.midiinfo</o> extends midiinfo. data.midiinfo offers a bpatcher with automatic input/output menu selections and named forward to send all its menu selections to. Match any receive object to your MIDI objects and data.midiinfo enables full control of your project's MIDI
  </description>
  <metadatalist>
    <metadata name='author'>
      Jon Bellona
    </metadata>
    <metadata name='tag'>
      controller
    </metadata>
    <metadata name='tag'>
      data
    </metadata>
  </metadatalist>
  <!--INLETS-->
  <inletlist>
    <inlet id="0" type="string">
      <digest>
        Extends midiinfo
      </digest>
      <description>
        TEXT_HERE
      </description>
    </inlet>
  </inletlist>
  <!--OUTLETS-->
  <outletlist/>
  <!--ATTRIBUTES-->
  <attributelist>
    <attribute name="send" get="1" set="1" size="1" type="string">
      <digest>
        The name of the forward object in data.midiinfo
      </digest>
      <description>
        @send _name_ names a forward object inside data.midiinfo that all MIDI menu selections are sent to.
      </description>
    </attribute>
  </attributelist>
