  <objarglist>
    <objarg name="input" optional="1" type="string">
      <digest>
        Fills a menu with MIDI input device names
      </digest>
      <description>
        'input' or 'output' changes the type of midi port menu.
      </description>
    </objarg>
    <objarg name="output" optional="1" type="string">
      <digest>
        Fills a menu with MIDI output device names
      </digest>
      <description>
        'input' or 'output' changes the type of midi port menu.
      </description>
    </objarg>
